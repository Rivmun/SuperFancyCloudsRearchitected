{
  "schemaVersion": 1,
  "id": "sfcr",
  "version": "${modVersion}",

  "name": "${modName}",
  "description": "${modDescription}",
  "authors": ["${modAuthor}"],
  "contact": {
    "homepage": "${modIssueTracker}"
  },

  "license": "${modLicense}",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": ["com.rimo.sfcr.SFCReFabric"],
    "client": ["com.rimo.sfcr.SFCReFabric"],
    "server": ["com.rimo.sfcr.SFCReFabric"],
    "modmenu": ["com.rimo.sfcr.config.SFCReModMenuIntegration"]
  },
  "mixins": [
    "sfcr-common.mixins.json"
  ],
  "depends": {
    "fabricloader": "${fabricLoaderRange}",
    "minecraft": "${fabricMinecraftVersionRange}",
    "architectury": ">=${archApiVersion}"
  },
  "suggests": {
    "cloth-config2":">=${clothVersion}",
    "modmenu": ">=11.0.0"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.modrinth": "https://modrinth.com/mod/superfancyclouds-refabricated",
        "MCMOD.cn": "https://www.mcmod.cn/class/9053.html"
      }
    }
  }
}
